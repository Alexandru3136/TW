@{
     ViewBag.Title = "Portofolio1";
     Layout = "~/Views/Shared/_Layout.cshtml";
}

@using System.Web.Optimization

<!-- Portfolio section  -->
<div class="portfolio-2-section">
     <ul class="portfolio-filter pf-2 controls">
          <li class="control" data-filter="all">Toate</li>
          <li class="control" data-filter=".nature">Natura</li>
          <li class="control" data-filter=".studio">Studio de fotografii</li>
          <li class="control" data-filter=".weddings">Nunți</li>
          <li class="control" data-filter=".lifestyle">Stiluri de viață</li>
          <li class="control" data-filter=".fashion">Fashion</li>
     </ul>
     <div class="portfolio-warp">
          <div class="row portfolio-gallery m-0">

               <div class="mix col-lg-4 col-sm-6 p-0 nature">
                    <div class="portfolio-box">
                         <div class="portfolio-item set-bg" data-setbg="../Content/themes/base/images/portfolio/1.jpg"></div>
                    </div>
               </div>

               <div class="mix col-lg-4 col-sm-6 p-0 studio">
                    <div class="portfolio-box">
                         <div class="portfolio-item set-bg" data-setbg="../Content/themes/base/images/portfolio/2.jpg"></div>
                    </div>
               </div>
               <div class="mix col-lg-4 col-sm-6 p-0 weddings">
                    <div class="portfolio-box">
                         <div class="portfolio-item set-bg" data-setbg="../Content/themes/base/images/portfolio/3.jpg"></div>
                    </div>
               </div>
               <div class="mix col-lg-4 col-sm-6 p-0 lifestyle">
                    <div class="portfolio-box">
                         <div class="portfolio-item set-bg" data-setbg="../Content/themes/base/images/portfolio/6.jpg"></div>
                    </div>
               </div>
               <div class="mix col-lg-4 col-sm-6 p-0 fashion">
                    <div class="portfolio-box">
                         <div class="portfolio-item set-bg" data-setbg="../Content/themes/base/images/portfolio/4.jpg"></div>
                    </div>
               </div>
               <div class="mix col-lg-4 col-sm-6 p-0 nature">
                    <div class="portfolio-box">
                         <div class="portfolio-item set-bg" data-setbg="../Content/themes/base/images/portfolio/5.jpg"></div>
                    </div>
               </div>
               <div class="mix col-lg-4 col-sm-6 p-0 studio">
                    <div class="portfolio-box">
                         <div class="portfolio-item set-bg" data-setbg="../Content/themes/base/images/portfolio/8.jpg"></div>
                    </div>
               </div>
               <div class="mix col-lg-4 col-sm-6 p-0 weddings">
                    <div class="portfolio-box">
                         <div class="portfolio-item set-bg" data-setbg="../Content/themes/base/images/portfolio/10.jpg"></div>
                    </div>
               </div>
               <div class="mix col-lg-4 col-sm-6 p-0 lifestyle">
                    <div class="portfolio-box">
                         <div class="portfolio-item set-bg" data-setbg="../Content/themes/base/images/portfolio/13.jpg"></div>
                    </div>
               </div>
          </div>
     </div>
</div>
